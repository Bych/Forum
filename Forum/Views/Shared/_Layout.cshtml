<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/CSS/squarely/default.css")" type="text/css" rel="Stylesheet" />
    
    
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

    @RenderSection("Head", false)
</head>
<body>
    <div id="wrapper">
    <div class="page">
        <header id="header">
            <div id="title">
                <h1>Star Wars forum</h1>
            </div>
            <div id="logindisplay">
                @Html.Partial("_LogOnPartial")
            </div>
            <div id="menu">
                <ul>
                    <li>@Html.ActionLink("Home", "Index", "Theme")</li>
                    <li>@Html.ActionLink("About", "About", "Theme")</li>
                    <li>@Html.ActionLink("Resume", "Upload", "Resume")</li>
                    <li>@Html.ActionLink("Resume list", "ResumeList", "Resume")</li>
                    @*<li><a href="../../TestPage.htm">Test page</a></li>*@
                </ul>
            </div>
        </header>
        <div id="page">
            <div>
                @RenderBody()
            </div>
        </div>
        <footer>
        </footer>
    </div>
</div>
</body>
</html>
